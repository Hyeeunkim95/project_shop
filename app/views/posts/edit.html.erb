<div class="container">
    <div class="row"> 
        <div class="card col s12"> 
            <div class="card-content"> 
                <div class="card-title">글 수정하기</div> 
                
                
                
                
                
                
                <form action="/posts/update/<%= @post.id %>" method="post">
                    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
                    
                    <input type="text" name="content" value="<%= @post.content %>"></input></br>
                    <input type="hidden" value="<%= @post.archived.id %>" name="archived_id"></input>
                    <input type="submit" value="수정하기"></input>
                </form>
                
                
                
                
                
                
                
            </div> 
        </div> 
    </div> 
</div>
